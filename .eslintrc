{
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  },
  "globals": {
    "fixture": "readonly",
    "test": "readonly"
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "sourceType": "module"
  },
  "rules" : {
    "max-len": ["error", {
      "code": 150
    }],
    "arrow-parens": ["error", "as-needed"],
    "no-multi-assign": 0,
    "strict": 0,
    "no-console": 0,
    "prefer-destructuring": 0,
    "function-paren-newline": 0,
    "global-require": 0,
    "prefer-spread": 0,
    "prefer-rest-params": 0,
    "prefer-arrow-callback": 0,
    "arrow-body-style": 0,
    "no-restricted-globals": 0,
    "consistent-return": 0,
    "no-param-reassign": 0,
    "no-underscore-dangle": 0,
    "import/no-unresolved": 0,
    "import/no-dynamic-require": 0,
    "import/no-extraneous-dependencies": 0
  }
}
